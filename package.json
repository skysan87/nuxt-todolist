{
  "name": "nuxt-todolist",
  "version": "0.9.0",
  "description": "Nuxt.js To-Do list",
  "author": "skysan",
  "private": true,
  "scripts": {
    "run-dev": "NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt dev",
    "run-prod": "nuxt generate && NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt start",
    "release-build": "nuxt build && nuxt generate",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/toast": "^3.3.1",
    "dayjs": "^1.10.8",
    "firebase": "^9.6.10",
    "nuxt": "^2.15.8",
    "v-calendar": "^2.4.1",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@vue/test-utils": "^1.3.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.11.0",
    "eslint-plugin-nuxt": ">=3.1.0",
    "firebase-tools": "^10.6.0",
    "jest": "^27.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^10.2.1",
    "vue-jest": "^3.0.7"
  },
  "overrides": {
    "minimist@1.2.5": "^1.2.6"
  }
}
