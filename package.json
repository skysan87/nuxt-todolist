{
  "name": "nuxt-todolist",
  "version": "0.12.3",
  "description": "Nuxt.js To-Do list",
  "author": "skysan",
  "private": true,
  "scripts": {
    "run-dev": "NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt dev",
    "dev-emulator-start": "firebase emulators:start --import=emulator_data --export-on-exit=emulator_data",
    "run-prod": "nuxt generate && NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt start",
    "release-build": "nuxt build && nuxt generate",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/toast": "^3.3.1",
    "dayjs": "^1.11.6",
    "firebase": "^9.13.0",
    "nuxt": "^2.15.8",
    "v-calendar": "^2.4.1",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^11.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "eslint": "^8.26.0",
    "eslint-plugin-nuxt": ">=4.0.0",
    "firebase-tools": "^11.16.0",
    "regenerator-runtime": "^0.13.10",
    "sass": "^1.49.9",
    "sass-loader": "^10.3.1"
  }
}
