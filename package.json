{
  "name": "nuxt-todolist",
  "version": "0.6.0",
  "description": "Nuxt.js To-Do list",
  "author": "skysan",
  "private": true,
  "scripts": {
    "run-dev": "NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt dev",
    "run-prod": "nuxt generate && NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt start",
    "release-build": "nuxt build && nuxt generate",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nuxtjs/axios": "^5.13.0",
    "@nuxtjs/device": "^2.0.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/toast": "^3.3.1",
    "dayjs": "^1.10.6",
    "firebase": "^8.2.5",
    "nuxt": "^2.14.12",
    "v-calendar": "^2.2.3",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "jest": "^26.6.3",
    "node-sass": "^7.0.0",
    "sass-loader": "^10.1.1",
    "vue-jest": "^4.0.0-beta.3"
  }
}
